        <!-- Container fluid  Kreiraj Korisnika -forma-->
    <div class="container-fluid">
        <div class="row">
            <div class="col-7 offset-2">
            <div class="card">
                <form class="form-horizontal" action="/admin/create/save"  method="POST">
                  <div class="card-body">
                    <h4 class="card-title">Lični podaci</h4>
                    <div class="form-group row">
                      <label for="fname"
                        class="col-sm-3 text-end control-label col-form-label">Ime</label>
                      <div class="col-sm-9">
                        <input type="text" name="first_name" class="form-control" id="first_name" placeholder="Ime"/>
                      </div>
                    </div>
                    <div class="form-group row">
                      <label for="lname"
                        class="col-sm-3 text-end control-label col-form-label">Prezime</label>
                      <div class="col-sm-9">
                        <input type="text" name="last_name" class="form-control" id="last_name" placeholder="Prezime"/>
                      </div>
                    </div>
                    <div class="form-group row">
                      <label for="lname" class="col-sm-3 text-end control-label col-form-label">Password</label>
                      <div class="col-sm-9">
                        <input type="password" name="password" class="form-control" id="password" placeholder="Password"/>
                      </div>
                    </div>
                    <div class="form-group row">
                      <label for="cono1" class="col-sm-3 text-end control-label col-form-label">Uloga</label>
                      <div class="col-sm-9">
                        <select name="role" class="form-control">
                            <option value="operater">Operater</option>
                            <option value="savetnik">Savjetnik</option>
                            <option value="admin">Admin</option>
                        </select>
                      </div>
                    </div>
                  </div>
                  <div class="border-top">
                    <div class="card-body">
                      <button type="submit" class="btn btn-primary">Kreiraj</button>
                  </div>
                </div>
              </form>
            </div>
        </div>
    </div>
