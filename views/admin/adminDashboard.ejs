      $(document).ready(function() {
        $(".delete-proizvod-btn").on('click', function () {
          let id = $(this).attr('data-id');
          console.log("ID:", id);
    
          let potvrda = confirm("Da li ste sigurni?");
          if (potvrda) {
            jQuery.ajax({
              url: `/admin/delete/proizvod/${id}`,
              method: 'GET', 
              dataType: 'json', 
            })
            .done(function(res) {
              console.log("Uspešan odgovor:", res);
              if (res === "Ok") {
                window.location.href = "/admin";
              }
            })
            .fail(function(jqXHR, textStatus, errorThrown) {
              console.log("Neuspešan zahtev:", textStatus, errorThrown);
            });
          }
        });
      });
    </script>
    

  </body>
</html>
